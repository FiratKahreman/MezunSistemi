<!DOCTYPE html>
<html lang="en">
<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>Karadeniz Teknik Üniversitesi Mezun Sistemi</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- site icon -->
    <link rel="icon" href="~/dosyalar/images/fevicon.png" type="image/png" />
    <!-- bootstrap css -->
    <link rel="stylesheet" href="~/dosyalar/css/bootstrap.min.css" />
    <!-- site css -->
    <link rel="stylesheet" href="~/dosyalar/css/style.css" />
    <!-- responsive css -->
    <link rel="stylesheet" href="~/dosyalar/css/responsive.css" />
    <!-- color css -->
    <link rel="stylesheet" href="~/dosyalar/css/colors.css" />
    <!-- select bootstrap -->
    <link rel="stylesheet" href="~/dosyalar/css/bootstrap-select.css" />
    <!-- scrollbar css -->
    <link rel="stylesheet" href="~/dosyalar/css/perfect-scrollbar.css" />
    <!-- custom css -->
    <link rel="stylesheet" href="~/dosyalar/css/custom.css" />
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="dashboard dashboard_1">
    <div class="full_container">
        <div class="inner_container">
            <!-- Sidebar  -->
            <nav id="sidebar">
                <div class="sidebar_blog_1">
                    <div class="sidebar-header">
                        <div class="logo_section">
                            <a href="index.html"><img class="logo_icon img-responsive" src="~/dosyalar/images/logo/ktulogobeyaz.jpg" alt="#" /></a>
                        </div>
                    </div>
                    <div class="sidebar_user_info">
                        <div class="icon_setting"></div>
                        <div class="user_profle_side">
                            <div class="user_img"><img class="img-responsive" src="~/dosyalar/images/layout_img/user_img.jpg" alt="#" /></div>
                            <div class="user_info">
                                <h6>Feridun Naptınnettinoğlu</h6>
                                <p><span class="online_animation"></span> Online</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="sidebar_blog_2">
                    <h4>General</h4>
                    <ul class="list-unstyled components">
                        <li><a href="widgets.html"><i class="fa fa-paper-plane white_color"></i> <span>Sosyal</span></a></li>
                        <li><a href="widgets.html"><i class="fa fa-map white_color"></i> <span>Harita</span></a></li>
                        <li><a href="widgets.html"><i class="fa fa-credit-card white_color"></i> <span>Mezun Kart</span></a></li>                                                                                     
                        <li><a href="settings.html"><i class="fa fa-cog white_color"></i> <span>Ayarlar</span></a></li>
                    </ul>
                </div>
            </nav>
            <!-- end sidebar -->

            <!-- right content -->
            <div id="content">
                <!-- topbar -->
                <div class="topbar">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <div class="full">
                            <button type="button" id="sidebarCollapse" class="sidebar_toggle"><i class="fa fa-bars"></i></button>
                            <div class="fa graduation-cap white_color">
                                <a href="index.html"><span class="baslik">KTÜ MEZUN SİSTEMİ</span></a>
                            </div>
                            <div class="right_topbar">
                                <div class="icon_info">
                                    <ul>
                                        <li><a href="#"><i class="fa fa-bell-o"></i><span class="badge">2</span></a></li>
                                        <li><a href="#"><i class="fa fa-question-circle"></i></a></li>
                                        <li><a href="#"><i class="fa fa-envelope-o"></i><span class="badge">3</span></a></li>
                                    </ul>
                                    <ul class="user_profile_dd">
                                        <li>
                                            <a class="dropdown-toggle" data-toggle="dropdown"><img class="img-responsive rounded-circle" src="~/dosyalar/images/layout_img/user_img.jpg" alt="#" /><span class="name_user">Feridun Naptınnettinoğlu</span></a>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" href="profile.html">Profilim</a>
                                                <a class="dropdown-item" href="settings.html">Ayarlar</a>
                                                <a class="dropdown-item" href="help.html">Yardım</a>
                                                <a class="dropdown-item" href="#"><span>Çıkış Yap</span> <i class="fa fa-sign-out"></i></a>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </nav>
                </div>
                <!-- end topbar -->
                
    <!-- dashboard inner -->
                <div class="midde_cont">
                    <div class="container-fluid">
                        <div class="row column_title">
                            <div class="col-md-12">
                                <div class="page_title">
                                    <h2>@ViewBag.Title</h2>
                                </div>
                            </div>
                        </div>
                        @RenderBody()                          
                        </div>
                        
                    </div>
                    <!-- footer -->
                    <div class="container-fluid">
                        <div class="footer">
                            <p>
                                Karadeniz Teknik Üniversitesi Mezun Sistemi  <b>2022</b>
                            </p>
                        </div>
                    </div>
                
                <!-- end dashboard inner -->
            </div>
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/dosyalar/js/jquery.min.js"></script>
    <script src="~/dosyalar/js/popper.min.js"></script>
    <script src="~/dosyalar/js/bootstrap.min.js"></script>
    <!-- wow animation -->
    <script src="~/dosyalar/js/animate.js"></script>
    <!-- select country -->
    <script src="~/dosyalar/js/bootstrap-select.js"></script>
    <!-- owl carousel -->
    <script src="~/dosyalar/js/owl.carousel.js"></script>
    <!-- chart js -->
    <script src="~/dosyalar/js/Chart.min.js"></script>
    <script src="~/dosyalar/js/Chart.bundle.min.js"></script>
    <script src="~/dosyalar/js/utils.js"></script>
    <script src="~/dosyalar/js/analyser.js"></script>
    <!-- nice scrollbar -->
    <script src="~/dosyalar/js/perfect-scrollbar.min.js"></script>
    <script>
        var ps = new PerfectScrollbar('#sidebar');
    </script>
    <!-- custom js -->
    <script src="~/dosyalar/js/chart_custom_style1.js"></script>
    <script src="~/dosyalar/js/custom.js"></script>
</body>
</html>