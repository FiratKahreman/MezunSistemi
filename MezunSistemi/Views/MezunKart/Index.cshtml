
@{
    ViewBag.Title = "Mezun Kart";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row column4 graph">
    <div class="col-md-6">
        <div class="dash_blog">
            <div class="dash_blog_inner">
                <div class="dash_head">
                    <h3><span><i class="fa fa-calendar"></i> @DateTime.Now.ToShortDateString()</span><span class="plus_green_bt"><a href="#">+</a></span></h3>
                </div>
                <div class="list_cont">
                    <p>Son Harcamaların</p>
                </div>
                <div class="task_list_main">
                    <ul class="task_list">
                        <li><a href="#">Ortahisar Kırtasiye</a><br><strong>01.01.2022 10:32</strong></li>
                        <li><a href="#">Naf Naf Giyim</a><br><strong>29.12.2021 20:00</strong></li>
                        <li><a href="#">Ganita Kitabevi</a><br><strong>25.12.2021 18:02</strong></li>
                        <li><a href="#">Üniversite Kafe</a><br><strong>25.12.2021 15:28</strong></li>
                        <li><a href="#">Konaklar Ev Yemekleri</a><br><strong>17.11.2021 17.42</strong></li>
                    </ul>
                </div>
                <div class="read_more">
                    <div class="center"><a class="main_bt read_bt" href="#">Read More</a></div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="white_shd full">
            <div class="widget_progress_bar">
                <p class="progress_no">1/10</p>
                <p class="progress_head">10 İndirim Hakkınızdan <strong>1</strong>'ini kullandınız.</p>
                <div class="progress_bar">

                    <span class="skill" style="width:10%;"><span class="info_valume">10%</span></span>
                    <div class="progress skill-bar ">
                        <div class="progress-bar progress-bar-animated progress-bar-striped" role="progressbar" aria-valuenow="73" aria-valuemin="0" aria-valuemax="100" style="width: 10%;"></div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4">
                <div class="full white_shd margin_bottom_30">
                    <div class="info_people">
                        <div class="p_info_img">
                            <img src="~/dosyalar/images/logo/mezunkarttaslak.png" alt="#" />
                        </div>
                        <div class="user_info_cont">
                            <h4>KTÜ Mezun Kart Kullanıcısısınız</h4>
                            
                            <p class="p_status">Aktif</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
